{
  "name": "@mfe-prototype/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target=serve --projects=host,dashboard,cart,shop,about --parallel --max-parallel=5",
    "api": "nx run-many --target=serve --projects=dashboard-api,cart-api,shop-api,about-api --parallel --max-parallel=5",
    "build": "nx run-many --target=build --configuration=production --projects=host,dashboard,cart,shop,about --parallel --max-parallel=5",
    "build-all": "nx run-many --target=build --configuration=production --all --parallel --max-parallel=5",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "format": "nx format:check",
    "pre:copy": "bash copy-remotes.sh",
    "pre:setup": "bash setup-deploy.sh",
    "pre:deploy": "npm run build && npm run pre:copy && npm run pre:setup",
    "deploy": "npm run pre:deploy && npx ngh --dir=dist/deploy --repo=https://x-access-token:${GITHUB_TOKEN}@github.com/Thompson632/mfe-prototype.git --no-silent --name='CI' --email='robertdavidthompson632@gmail.com'"
  },
  "private": true,
  "devDependencies": {
